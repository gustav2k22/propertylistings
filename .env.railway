# Railway environment variables
NODE_ENV=production

# Server configuration
PORT=3000

# Fallback database configuration (only used if Railway MySQL variables are not set)
# These will be overridden by Railway's automatically set MySQL variables
DB_HOST=localhost
DB_USER=property_user
DB_PASSWORD=property123
DB_NAME=property_listings
DB_PORT=3306
DB_SSL=true

# Force the app to use the fallback data when running locally
USE_MOCK_DATA=true
